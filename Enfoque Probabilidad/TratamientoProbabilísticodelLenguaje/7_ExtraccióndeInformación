import spacy

# Cargar el modelo de spaCy para ingl√©s
nlp = spacy.load("en_core_web_sm")

# Texto de ejemplo
text = "Apple is looking at buying U.K. startup for $1 billion."

# Procesar el texto
doc = nlp(text)

# Extraer entidades nombradas
for ent in doc.ents:
    print(f"Entidad: {ent.text}, Tipo: {ent.label_}")
